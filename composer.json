{
    "name": "mtownsend/request-xml",
    "description": "The missing XML support for Laravel's Request class.",
    "keywords": [
        "request",
        "laravel",
        "xml",
        "isXml",
        "wantsXml",
        "api"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mark Townsend",
            "email": "mtownsend5512@gmail.com",
            "role": "Developer"
        }
    ],
	"autoload": {
        "psr-4": {
            "Mtownsend\\RequestXml\\": "src"
        }
    },
    "require": {
        "php": "~7.0|~8.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0|^7.0|^8.0",
        "illuminate/http": "~5.5.0|~5.6.0|~5.7.0|~5.8.0|~6.0|^7.0|^8.0",
        "mtownsend/xml-to-array": "^1.0|^2.0"
    },
    "require-dev": {
      "phpunit/phpunit": "^6.4|^8.5"
    },
    "autoload-dev": {
        "psr-4": {
            "Mtownsend\\RequestXml\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mtownsend\\RequestXml\\Providers\\RequestXmlServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable"
}
